# Настройки Telegram
# Получить токен у @BotFather
TGSERVER__TOKEN=your_telegram_bot_token_here
# Ссылка от localtunnel + /webhook (например: https://my-bot.loca.lt/webhook)
TGSERVER__WEBHOOK_URL=https://your-domain.loca.lt/webhook

# Настройки БД (Postgres в Docker)
DB__NAME=rag_bot_db
DB__USER=rag_user
DB__PASS=strongpassword123
DB__HOST=postgres
DB__PORT=5432

# Настройки LLM
LLM_PROVIDER=ollama
OLLAMA_HOST=http://ollama:11434
# Модель должна совпадать с той, что качается в setup.sh
OLLAMA_MODEL=llama3.1

# Настройки RAG
EMBEDDING_MODEL_NAME=intfloat/multilingual-e5-large